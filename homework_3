#2. Добавить необходимую таблицу/таблицы для того, чтобы можно было использовать лайки для медиафайлов, постов и
#   пользователей.

-- Таблица с автором лайка, пользователем, получившим лайк и постом с лайком---------------------------------------------
CREATE TABLE likes (
id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
from_user INT UNSIGNED NOT NULL COMMENT "Ссылка на пользователя поставившего лайк",
to_user INT UNSIGNED NOT NULL COMMENT "Ссылка на получателя лайка",
message_id INT UNSIGNED NOT NULL COMMENT "Ссылка на пост с лайком",
date_of_like DATETIME COMMENT "Время лайка", 
created_at DATETIME DEFAULT CURRENT_TIMESTAMP COMMENT "Время создания строки",  
updated_at DATETIME DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT "Время обновления строки"
) COMMENT "Получатель лайка"; 

ALTER TABLE likes ADD CONSTRAINT fk_like_user_from FOREIGN KEY (from_user) REFERENCES users (id);
ALTER TABLE likes ADD CONSTRAINT fk_like_user_to FOREIGN KEY (to_user) REFERENCES users (id);
ALTER TABLE likes ADD CONSTRAINT fk_message_id FOREIGN KEY (message_id) REFERENCES messages (id);

